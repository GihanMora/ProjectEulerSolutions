def p(y):
    for i in range(2,y):
        if y%i==0:
            return False
    else:
        return True
k=[]
for j in range(1000):
    if p(j):k.append(j)
    

def q(c,w):
    s=0
    for n in range(0,1000):
        if p(n**2 +(c)*n +w ):
            s+=1
    return s
