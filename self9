j=[]

for i in range(1,50000):
    if int(i**0.5)*int(i**0.5)==int(i):
        j.append(i)

m=[]
for t in range(1,1200):
    c=0
    for n in j[::-1]:
        for k in j:
            if (n-k) in j:
                if n**0.5+k**0.5+(n-k)**0.5==1000:
                    print n**0.5,k**0.5,(n-k)**0.5

         
             
             
                
        



        
                 
    
